<style>
    #content {
        white-space: inherit;
    }
    a.btn-primary,
    a.btn-primary:hover {
        color: #eee;
    }
</style>

<div class="pal">
    <div class="panel panel-info">
        <div class="panel-heading Bg-secondary">
            <h3 class="panel-title"><i class="i-project"></i> &nbsp;Create a New Project</h3>
        </div>
        <div class="panel-body bg-white">
            <form class="form-horizontal">
                <fieldset>
                    <legend>Basic Project Information</legend>
                    <div class="form-group">
                        <label for="inputEmail" class="col-lg-2 control-label">Project Type</label>
                        <div class="col-lg-10">
                            <p style="margin-top:-3px">
                              <div class="label label-primary">Mailbox</div>
                            </p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputName" class="col-lg-2 control-label">Project Name</label>
                        <div class="col-lg-10">
                            <input type="text" class="form-control" id="inputName" placeholder="Enter a name for your project" style="background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;">
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-lg-12 text-right">
                            <a href="projects.html" class="btn btn-default">Cancel</a>
                            <a href="source-settings.html" class="btn btn-primary">Next</a>
                        </div>
                    </div>
                </fieldset>
            </form>
        </div>
    </div>

</div>

