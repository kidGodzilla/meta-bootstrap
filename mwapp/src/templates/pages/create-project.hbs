    }
    .accordion .list-group-item hr,
    .accordion .list-group-item .btn {
        display: none;
    }
    .accordion .list-group-item.open {
        background: #fff;
        padding: 10px 15px;
    }
    .accordion .list-group-item.open hr{
        display: block;
    }
    .accordion .list-group-item.open .btn {
        display: inline;
    }
    .accordion header {
        cursor: pointer;
    }
